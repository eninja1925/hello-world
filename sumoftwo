// To find the sum of two integers
#include <iostream>
using namespace std;
int main()

{
    int number1;
    int number2;
    int total;

    number1 = 50;
    number2 = 100;

    total = number1 + number2;

    cout << "The Total is " << total << endl;
    system("pause");

    return 0;
}
